<link rel="stylesheet" type="text/css" href="../assets/login.css"/>

<div class="logo"></div>
<div class="login-block">
      <h1>Login</h1> 

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Login" %>

<% end %>
